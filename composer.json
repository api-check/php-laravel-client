{
    "name": "apicheck/laravel",
    "description": "A wrapper around ApiCheck get to validate addresses.",
    "keywords": [
        "apicheck",
        "laravel",
        "addressvalidatione",
        "api"
    ],
    "homepage": "https://github.com/api-check/laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Wouter Doornbos",
            "email": "w.doornbos@apicheck.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^6.4|^7.0",
        "illuminate/support": "5.8.*|6.*|7.*|8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*|4.*|5.*|6.*",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "GNAHotelSolutions\\Weather\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GNAHotelSolutions\\Weather\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApiCheck\\Laravel\\ApiCheckServiceProvider"
            ],
            "aliases": {
                "Weather": "ApiCheck\\Laravel\\ApiCheckFacade"
            }
        }
    }
}
