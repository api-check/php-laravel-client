{
  "name": "api-check/php-laravel-client",
  "description": "ApiCheck wrapper for Laravel",
  "keywords": [
    "apicheck",
    "laravel",
    "addressvalidation",
    "postalcodeapi",
    "addressapi"
  ],
  "homepage": "https://apicheck.nl/",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "ApiCheck",
      "email": "support@apicheck.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.4|^7.0",
    "apicheck/php-client": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "3.8.*|4.*|5.*|6.*",
    "phpunit/phpunit": "^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "ApiCheck\\Api\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable" : true,
  "extra": {
    "laravel": {
      "providers": [
        "ApiCheck\\Laravel\\ServiceProvider"
      ],
      "aliases": {
        "ApiCheck": "ApiCheck\\Laravel\\Facades\\ApiCheck"
      }
    }
  }
}
